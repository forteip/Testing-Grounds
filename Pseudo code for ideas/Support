int distance_finder(int arr1[],int arr2[]){
//player distance compared to other
	int x_distance;
	int y_distance;
	x_distance = arr2[0] - arr1[0];
	y_distance = arr2[1] - arr1[1];
	return (x_distance + y_distance);
}

int attack(int hp, int atk){
return (hp - atk);
}

boolean dodge(int chance){
	int roll;
	roll = rand(100);
	if (roll < chance){
		return true;
	}
	else {
		return false;
	}
}

